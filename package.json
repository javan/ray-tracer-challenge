{
  "name": "ray-tracer-challenge",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "build": "webpack",
    "now-build": "rm -rf dist && webpack -p && cp -R public dist && yarn test --verbose 2>&1 > dist/tests.txt",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "dependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "ava": "2.4.0",
    "babel-loader": "8.0.6",
    "express": "4.17.1",
    "faster.js": "1.1.1",
    "stimulus": "1.1.1",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2",
    "worker-loader": "2.0.0"
  },
  "ava": {
    "failFast": true,
    "compileEnhancements": false,
    "babel": false,
    "cache": false,
    "files": [
      "test/**/*.test.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "require": [
      "esm"
    ]
  }
}
